{
  "compilerOptions": {
    "allowJs": true,
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "NodeNext",
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": [
      "ESNext",
      "DOM"
    ],
    "skipLibCheck": true,
    "typeRoots": [
      "./types",
      "./node_modules/@types",
      "./node_modules/vue/dist/vue.d.ts",
      "./node_modules/idb-keyval/dist/index.d.ts"
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@ventose/ui": [
        "./src/devui/ui/index.tsx"
      ]
    }
  },
}